{
  "Logging": {
    "LogLevel": { //默认的日志配置
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning",
      "Super": "Information"
    },
    "Log4Net": {//针对每个日志提供程序作个性化配置
      "LogLevel": {
        "Default": "Warning",
        "Super": "Information"
      }
    },
    "NLog": {
      "LogLevel": {
        "Default": "Warning",
        "Super": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
