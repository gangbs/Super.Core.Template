{
  "Logging": {
    "LogLevel": { //默认的日志配置
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning",
      "Pims": "Information"
    },
    "Console": { //针对每个日志提供程序作个性化配置
      "IncludeScopes": true,
      "LogLevel": {}
    },

    "Log4Net": {
      "LogLevel": {
        "Default": "Warning",
        "Pims": "Information"
      }
    },
    "NLog": {
      "LogLevel": {
        "Default": "Warning",
        "Pims": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
